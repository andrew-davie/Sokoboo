     28  6b84							; PLANNED PLAYER VARIANTS:
     29  6b84							; - RAM, speed, player ROM: c0/c1 patterns have same length
     30  6b84							; - RAM: Pack 2 values (out of cur_pat_index, cur_note_index, envelope_index)
     31  6b84							;	 into one and use lsr/asl to unpack them, allowing only ranges of
     32  6b84							;	 16/16 or 32/8 for them, depending on number of patterns, max
     33  6b84							;	 pattern size and max ADSR size
     34  6b84							; - ROM: Check if tt_SequenceTable can hold ptrs directly without indexing
     35  6b84							;	 tt_PatternPtrLo/Hi. Can be smaller if not many patterns get repeated
     36  6b84							;	 (saves table and decode routine)
     37  6b84							; - Speed: Inline tt_CalcInsIndex
     38  6b84							; - Speed: Store ptr to current note in RAM instead of reconstructing it?
     39  6b84							;	 Might also save the need for cur_note_index
     40  6b84
     41  6b84
     42  6b84							; ---------------------------------------------------------------------
     43  6b84							; Helper macro: Retrieves current note. May advance pattern if needed.
     44  6b84							; Becomes a subroutine if TT_USE_OVERLAY is used.
     45  6b84							; ---------------------------------------------------------------------
     46  6b84					      MAC	tt_fetch_current_note
     47  6b84							; construct ptr to pattern
     48  6b84				   .constructPatPtr
     49  6b84					      ldy	tt_cur_pat_index_c0,x	; get current pattern (index into tt_SequenceTable)
     50  6b84					      lda	tt_SequenceTable,y
     51  6b84					      IF	TT_USE_GOTO = 1
     52  6b84					      bpl	.noPatternGoto
     53  6b84					      and	#%01111111	; mask out goto bit to get pattern number
     54  6b84					      sta	tt_cur_pat_index_c0,x	; store goto'ed pattern index
     55  6b84					      bpl	.constructPatPtr	; unconditional
     56  6b84				   .noPatternGoto
     57  6b84					      ENDIF
     58  6b84					      tay
     59  6b84					      lda	tt_PatternPtrLo,y
     60  6b84					      sta	tt_ptr
     61  6b84					      lda	tt_PatternPtrHi,y
     62  6b84					      sta	tt_ptr+1
     63  6b84							; get new note
     64  6b84					      IF	TT_USE_OVERLAY = 0
     65  6b84					      ldy	tt_cur_note_index_c0,x
     66  6b84					      ELSE
     67  6b84							; If the V flag is set and if the new note is an instrument,
     68  6b84							; it means it got pre-fetched by an overlay percussion, it has
     69  6b84							; to remain in sustain.
     70  6b84					      clv
     71  6b84							; check if note had been pre-fetched by overlay perc already
     72  6b84					      lda	tt_cur_note_index_c0,x
     73  6b84					      bpl	.notPrefetched
     74  6b84							; If so, remove flag
     75  6b84					      and	#%01111111
     76  6b84					      sta	tt_cur_note_index_c0,x
     77  6b84							; Set V flag for later
     78  6b84					      bit	tt_Bit6Set
     79  6b84				   .notPrefetched
     80  6b84					      tay
     81  6b84					      ENDIF
     82  6b84					      lda	(tt_ptr),y
     83  6b84							; pre-process new note
     84  6b84							; 7..5: instrument (1..7), 4..0 (0..31): frequency
     85  6b84							; 0/0: End of pattern
     86  6b84					      bne	.noEndOfPattern
     87  6b84							; End of pattern: Advance to next pattern
     88  6b84					      sta	tt_cur_note_index_c0,x	; a is 0
     89  6b84					      inc	tt_cur_pat_index_c0,x
     90  6b84					      bne	.constructPatPtr	; unconditional
     91  6b84				   .noEndOfPattern
     92  6b84					      ENDM
