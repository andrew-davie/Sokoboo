    118  7c7a				  -	      ELSE
    119  7c7a				  -	      beq	.pause
    120  7c7a				  -	      bcs	.newNote
    121  7c7a				  -
    122  7c7a				  -			; --- slide/hold ---
    123  7c7a				  -			; Adjust frequency and hold note in sustain.
    124  7c7a				  -			; composer/tracker has to make sure that no unwanted
    125  7c7a				  -			; under/overflow happens.
    126  7c7a				  -			; Note: f = f + (8-(16-x)) = x + f - 8
    127  7c7a				  -	      adc	tt_cur_ins_c0,x	; carry is clear after cmp
    128  7c7a				  -	      sec
    129  7c7a				  -	      sbc	#8
    130  7c7a				  -	      sta	tt_cur_ins_c0,x
    131  7c7a				  -	      bcs	.finishedNewNote	; unconditional, since legally no underflow can happen (ins>0 or HOLD for ins=0)
    132  7c7a					      ENDIF
    133  7c7a
    134  7c7a							; --- pause ---
    135  7c7a				   .pause
    136  7c7a							; Get release index for current instrument. Since a pause can
    137  7c7a							; only follow an instrument, we don't need to handle percussion
    138  7c7a							; or commands.
    139  7c7a		       b5 df		      lda	tt_cur_ins_c0,x
    140  7c7c		       20 15 fd 	      jsr	tt_CalcInsIndex
