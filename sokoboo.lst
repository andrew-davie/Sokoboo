     28  d413							; PLANNED PLAYER VARIANTS:
     29  d413							; - RAM, speed, player ROM: c0/c1 patterns have same length
     30  d413							; - RAM: Pack 2 values (out of cur_pat_index, cur_note_index, envelope_index)
     31  d413							;	 into one and use lsr/asl to unpack them, allowing only ranges of
     32  d413							;	 16/16 or 32/8 for them, depending on number of patterns, max
     33  d413							;	 pattern size and max ADSR size
     34  d413							; - ROM: Check if tt_SequenceTable can hold ptrs directly without indexing
     35  d413							;	 tt_PatternPtrLo/Hi. Can be smaller if not many patterns get repeated
     36  d413							;	 (saves table and decode routine)
     37  d413							; - Speed: Inline tt_CalcInsIndex
     38  d413							; - Speed: Store ptr to current note in RAM instead of reconstructing it?
     39  d413							;	 Might also save the need for cur_note_index
     40  d413
     41  d413
     42  d413							; ---------------------------------------------------------------------
     43  d413							; Helper macro: Retrieves current note. May advance pattern if needed.
     44  d413							; Becomes a subroutine if TT_USE_OVERLAY is used.
     45  d413							; ---------------------------------------------------------------------
     46  d413					      MAC	tt_fetch_current_note
     47  d413							; construct ptr to pattern
     48  d413				   .constructPatPtr
     49  d413					      ldy	tt_cur_pat_index_c0,x	; get current pattern (index into tt_SequenceTable)
     50  d413					      lda	tt_SequenceTable,y
     51  d413					      IF	TT_USE_GOTO = 1
     52  d413					      bpl	.noPatternGoto
     53  d413					      and	#%01111111	; mask out goto bit to get pattern number
     54  d413					      sta	tt_cur_pat_index_c0,x	; store goto'ed pattern index
     55  d413					      bpl	.constructPatPtr	; unconditional
     56  d413				   .noPatternGoto
     57  d413					      ENDIF
     58  d413					      tay
     59  d413					      lda	tt_PatternPtrLo,y
     60  d413					      sta	tt_ptr
     61  d413					      lda	tt_PatternPtrHi,y
     62  d413					      sta	tt_ptr+1
     63  d413							; get new note
     64  d413					      IF	TT_USE_OVERLAY = 0
     65  d413					      ldy	tt_cur_note_index_c0,x
     66  d413					      ELSE
     67  d413							; If the V flag is set and if the new note is an instrument,
     68  d413							; it means it got pre-fetched by an overlay percussion, it has
     69  d413							; to remain in sustain.
     70  d413					      clv
     71  d413							; check if note had been pre-fetched by overlay perc already
     72  d413					      lda	tt_cur_note_index_c0,x
     73  d413					      bpl	.notPrefetched
     74  d413							; If so, remove flag
     75  d413					      and	#%01111111
     76  d413					      sta	tt_cur_note_index_c0,x
     77  d413							; Set V flag for later
     78  d413					      bit	tt_Bit6Set
     79  d413				   .notPrefetched
     80  d413					      tay
     81  d413					      ENDIF
     82  d413					      lda	(tt_ptr),y
     83  d413							; pre-process new note
     84  d413							; 7..5: instrument (1..7), 4..0 (0..31): frequency
     85  d413							; 0/0: End of pattern
     86  d413					      bne	.noEndOfPattern
     87  d413							; End of pattern: Advance to next pattern
     88  d413					      sta	tt_cur_note_index_c0,x	; a is 0
     89  d413					      inc	tt_cur_pat_index_c0,x
