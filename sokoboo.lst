    183  d09f		       e0 07		      cpx	#SCREEN_LINES-1	; might become variable when vertical scrolling
    184  d0a1		       d0 07		      bne	.skipPatch
    185  d0a3		       a9 60		      lda	#$60	; rts
    186  d0a5		       a0 41		      ldy	#<SELFMOD_X
