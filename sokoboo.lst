    114  d418		       c9 10		      cmp	#TT_INS_PAUSE
    115  d41a					      IF	TT_USE_SLIDE = 0
    116  d41a		       90 55		      bcc	.finishedNewNote
    117  d41c		       d0 3d		      bne	.newNote
    118  d41e				  -	      ELSE
    119  d41e				  -	      beq	.pause
    120  d41e				  -	      bcs	.newNote
    121  d41e				  -
    122  d41e				  -			; --- slide/hold ---
    123  d41e				  -			; Adjust frequency and hold note in sustain.
    124  d41e				  -			; composer/tracker has to make sure that no unwanted
    125  d41e				  -			; under/overflow happens.
    126  d41e				  -			; Note: f = f + (8-(16-x)) = x + f - 8
    127  d41e				  -	      adc	tt_cur_ins_c0,x	; carry is clear after cmp
    128  d41e				  -	      sec
    129  d41e				  -	      sbc	#8
    130  d41e				  -	      sta	tt_cur_ins_c0,x
    131  d41e				  -	      bcs	.finishedNewNote	; unconditional, since legally no underflow can happen (ins>0 or HOLD for ins=0)
    132  d41e					      ENDIF
    133  d41e
    134  d41e							; --- pause ---
    135  d41e				   .pause
    136  d41e							; Get release index for current instrument. Since a pause can
    137  d41e							; only follow an instrument, we don't need to handle percussion
    138  d41e							; or commands.
    139  d41e		       b5 d7		      lda	tt_cur_ins_c0,x
    140  d420		       20 b9 f4 	      jsr	tt_CalcInsIndex
