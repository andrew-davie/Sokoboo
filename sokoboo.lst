     28  6b76							; PLANNED PLAYER VARIANTS:
     29  6b76							; - RAM, speed, player ROM: c0/c1 patterns have same length
     30  6b76							; - RAM: Pack 2 values (out of cur_pat_index, cur_note_index, envelope_index)
     31  6b76							;	 into one and use lsr/asl to unpack them, allowing only ranges of
     32  6b76							;	 16/16 or 32/8 for them, depending on number of patterns, max
     33  6b76							;	 pattern size and max ADSR size
     34  6b76							; - ROM: Check if tt_SequenceTable can hold ptrs directly without indexing
     35  6b76							;	 tt_PatternPtrLo/Hi. Can be smaller if not many patterns get repeated
     36  6b76							;	 (saves table and decode routine)
     37  6b76							; - Speed: Inline tt_CalcInsIndex
     38  6b76							; - Speed: Store ptr to current note in RAM instead of reconstructing it?
     39  6b76							;	 Might also save the need for cur_note_index
     40  6b76
     41  6b76
     42  6b76							; ---------------------------------------------------------------------
     43  6b76							; Helper macro: Retrieves current note. May advance pattern if needed.
     44  6b76							; Becomes a subroutine if TT_USE_OVERLAY is used.
     45  6b76							; ---------------------------------------------------------------------
     46  6b76					      MAC	tt_fetch_current_note
     47  6b76							; construct ptr to pattern
     48  6b76				   .constructPatPtr
     49  6b76					      ldy	tt_cur_pat_index_c0,x	; get current pattern (index into tt_SequenceTable)
     50  6b76					      lda	tt_SequenceTable,y
     51  6b76					      IF	TT_USE_GOTO = 1
     52  6b76					      bpl	.noPatternGoto
     53  6b76					      and	#%01111111	; mask out goto bit to get pattern number
     54  6b76					      sta	tt_cur_pat_index_c0,x	; store goto'ed pattern index
     55  6b76					      bpl	.constructPatPtr	; unconditional
     56  6b76				   .noPatternGoto
     57  6b76					      ENDIF
     58  6b76					      tay
     59  6b76					      lda	tt_PatternPtrLo,y
     60  6b76					      sta	tt_ptr
     61  6b76					      lda	tt_PatternPtrHi,y
     62  6b76					      sta	tt_ptr+1
     63  6b76							; get new note
     64  6b76					      IF	TT_USE_OVERLAY = 0
     65  6b76					      ldy	tt_cur_note_index_c0,x
     66  6b76					      ELSE
     67  6b76							; If the V flag is set and if the new note is an instrument,
     68  6b76							; it means it got pre-fetched by an overlay percussion, it has
     69  6b76							; to remain in sustain.
     70  6b76					      clv
     71  6b76							; check if note had been pre-fetched by overlay perc already
     72  6b76					      lda	tt_cur_note_index_c0,x
     73  6b76					      bpl	.notPrefetched
     74  6b76							; If so, remove flag
     75  6b76					      and	#%01111111
     76  6b76					      sta	tt_cur_note_index_c0,x
     77  6b76							; Set V flag for later
     78  6b76					      bit	tt_Bit6Set
     79  6b76				   .notPrefetched
     80  6b76					      tay
     81  6b76					      ENDIF
     82  6b76					      lda	(tt_ptr),y
     83  6b76							; pre-process new note
     84  6b76							; 7..5: instrument (1..7), 4..0 (0..31): frequency
     85  6b76							; 0/0: End of pattern
     86  6b76					      bne	.noEndOfPattern
     87  6b76							; End of pattern: Advance to next pattern
     88  6b76					      sta	tt_cur_note_index_c0,x	; a is 0
     89  6b76					      inc	tt_cur_pat_index_c0,x
     90  6b76					      bne	.constructPatPtr	; unconditional
     91  6b76				   .noEndOfPattern
     92  6b76					      ENDM
     93  6b76
