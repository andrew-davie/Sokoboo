     28  d40f							; PLANNED PLAYER VARIANTS:
     29  d40f							; - RAM, speed, player ROM: c0/c1 patterns have same length
     30  d40f							; - RAM: Pack 2 values (out of cur_pat_index, cur_note_index, envelope_index)
     31  d40f							;	 into one and use lsr/asl to unpack them, allowing only ranges of
     32  d40f							;	 16/16 or 32/8 for them, depending on number of patterns, max
     33  d40f							;	 pattern size and max ADSR size
     34  d40f							; - ROM: Check if tt_SequenceTable can hold ptrs directly without indexing
     35  d40f							;	 tt_PatternPtrLo/Hi. Can be smaller if not many patterns get repeated
     36  d40f							;	 (saves table and decode routine)
     37  d40f							; - Speed: Inline tt_CalcInsIndex
     38  d40f							; - Speed: Store ptr to current note in RAM instead of reconstructing it?
     39  d40f							;	 Might also save the need for cur_note_index
     40  d40f
     41  d40f
     42  d40f							; ---------------------------------------------------------------------
     43  d40f							; Helper macro: Retrieves current note. May advance pattern if needed.
     44  d40f							; Becomes a subroutine if TT_USE_OVERLAY is used.
     45  d40f							; ---------------------------------------------------------------------
     46  d40f					      MAC	tt_fetch_current_note
     47  d40f							; construct ptr to pattern
     48  d40f				   .constructPatPtr
     49  d40f					      ldy	tt_cur_pat_index_c0,x	; get current pattern (index into tt_SequenceTable)
     50  d40f					      lda	tt_SequenceTable,y
     51  d40f					      IF	TT_USE_GOTO = 1
     52  d40f					      bpl	.noPatternGoto
     53  d40f					      and	#%01111111	; mask out goto bit to get pattern number
     54  d40f					      sta	tt_cur_pat_index_c0,x	; store goto'ed pattern index
     55  d40f					      bpl	.constructPatPtr	; unconditional
     56  d40f				   .noPatternGoto
     57  d40f					      ENDIF
     58  d40f					      tay
     59  d40f					      lda	tt_PatternPtrLo,y
     60  d40f					      sta	tt_ptr
     61  d40f					      lda	tt_PatternPtrHi,y
     62  d40f					      sta	tt_ptr+1
     63  d40f							; get new note
     64  d40f					      IF	TT_USE_OVERLAY = 0
     65  d40f					      ldy	tt_cur_note_index_c0,x
     66  d40f					      ELSE
     67  d40f							; If the V flag is set and if the new note is an instrument,
     68  d40f							; it means it got pre-fetched by an overlay percussion, it has
     69  d40f							; to remain in sustain.
     70  d40f					      clv
     71  d40f							; check if note had been pre-fetched by overlay perc already
     72  d40f					      lda	tt_cur_note_index_c0,x
     73  d40f					      bpl	.notPrefetched
     74  d40f							; If so, remove flag
     75  d40f					      and	#%01111111
     76  d40f					      sta	tt_cur_note_index_c0,x
     77  d40f							; Set V flag for later
     78  d40f					      bit	tt_Bit6Set
     79  d40f				   .notPrefetched
     80  d40f					      tay
     81  d40f					      ENDIF
     82  d40f					      lda	(tt_ptr),y
     83  d40f							; pre-process new note
     84  d40f							; 7..5: instrument (1..7), 4..0 (0..31): frequency
     85  d40f							; 0/0: End of pattern
     86  d40f					      bne	.noEndOfPattern
     87  d40f							; End of pattern: Advance to next pattern
     88  d40f					      sta	tt_cur_note_index_c0,x	; a is 0
     89  d40f					      inc	tt_cur_pat_index_c0,x
     90  d40f					      bne	.constructPatPtr	; unconditional
     91  d40f				   .noEndOfPattern
     92  d40f					      ENDM
     93  d40f
