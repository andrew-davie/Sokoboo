      1  d42b
      2  d42b				   .constructPatPtr
      3  d42b		       b4 d1		      ldy	tt_cur_pat_index_c0,x
      4  d42d		       b9 0e f6 	      lda	tt_SequenceTable,y
      5  d430					      IF	TT_USE_GOTO = 1
      6  d430		       10 06		      bpl	.noPatternGoto
      7  d432		       29 7f		      and	#%01111111
      8  d434		       95 d1		      sta	tt_cur_pat_index_c0,x
      9  d436		       10 f3		      bpl	.constructPatPtr
