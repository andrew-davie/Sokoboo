     98  7c6b				   tt_Player  SUBROUTINE
     99  7c6b							; ==================== Sequencer ====================
    100  7c6b							; Decrease speed timer
    101  7c6b		       c6 d8		      dec	tt_timer
    102  7c6d		       10 6e		      bpl	.noNewNote
    103  7c6f
    104  7c6f							; Timer ran out: Do sequencer
    105  7c6f							; Advance to next note
    106  7c6f		       a2 01		      ldx	#1	; 2 channels
    107  7c71				   .advanceLoop
    108  7c71					      IF	TT_USE_OVERLAY = 1
    109  7c71		       20 87 fc 	      jsr	tt_FetchNote
    110  7c74				  -	      ELSE
    111  7c74				  -	      TT_FETCH_CURRENT_NOTE
    112  7c74					      ENDIF
