     12  d438		       a8		      tay
     13  d439		       b9 04 f6 	      lda	tt_PatternPtrLo,y
     14  d43c		       85 d9		      sta	tt_ptr
     15  d43e		       b9 09 f6 	      lda	tt_PatternPtrHi,y
     16  d441		       85 da		      sta	tt_ptr+1
     17  d443
     18  d443				  -	      IF	TT_USE_OVERLAY = 0
     19  d443				  -	      ldy	tt_cur_note_index_c0,x
     20  d443					      ELSE
     21  d443
     22  d443
     23  d443
     24  d443		       b8		      clv
     25  d444
     26  d444		       b5 d3		      lda	tt_cur_note_index_c0,x
     27  d446		       10 07		      bpl	.notPrefetched
     28  d448
     29  d448		       29 7f		      and	#%01111111
     30  d44a		       95 d3		      sta	tt_cur_note_index_c0,x
     31  d44c
     32  d44c		       2c bf f4 	      bit	tt_Bit6Set
     33  d44f				   .notPrefetched
     34  d44f		       a8		      tay
     35  d450					      ENDIF
     36  d450		       b1 d9		      lda	(tt_ptr),y
     37  d452
     38  d452
     39  d452
     40  d452		       d0 06		      bne	.noEndOfPattern
     41  d454
     42  d454		       95 d3		      sta	tt_cur_note_index_c0,x
     43  d456		       f6 d1		      inc	tt_cur_pat_index_c0,x
     44  d458		       d0 d1		      bne	.constructPatPtr
