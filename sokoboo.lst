    154  6b72				   tt_FetchNote
      0  6b72					      TT_FETCH_CURRENT_NOTE
      1  6b72
      2  6b72				   .constructPatPtr
      3  6b72		       b4 d2		      ldy	tt_cur_pat_index_c0,x
      4  6b74		       b9 55 f5 	      lda	tt_SequenceTable,y
      5  6b77					      IF	TT_USE_GOTO = 1
      6  6b77		       10 06		      bpl	.noPatternGoto
      7  6b79		       29 7f		      and	#%01111111
      8  6b7b		       95 d2		      sta	tt_cur_pat_index_c0,x
      9  6b7d		       10 f3		      bpl	.constructPatPtr
     10  6b7f				   .noPatternGoto
     11  6b7f					      ENDIF
     12  6b7f		       a8		      tay
     13  6b80		       b9 4b f5 	      lda	tt_PatternPtrLo,y
     14  6b83		       85 da		      sta	tt_ptr
     15  6b85		       b9 50 f5 	      lda	tt_PatternPtrHi,y
     16  6b88		       85 db		      sta	tt_ptr+1
     17  6b8a
     18  6b8a				  -	      IF	TT_USE_OVERLAY = 0
     19  6b8a				  -	      ldy	tt_cur_note_index_c0,x
     20  6b8a					      ELSE
     21  6b8a
     22  6b8a
     23  6b8a
     24  6b8a		       b8		      clv
     25  6b8b
     26  6b8b		       b5 d4		      lda	tt_cur_note_index_c0,x
     27  6b8d		       10 07		      bpl	.notPrefetched
     28  6b8f
     29  6b8f		       29 7f		      and	#%01111111
     30  6b91		       95 d4		      sta	tt_cur_note_index_c0,x
     31  6b93
     32  6b93		       2c 06 f4 	      bit	tt_Bit6Set
     33  6b96				   .notPrefetched
     34  6b96		       a8		      tay
     35  6b97					      ENDIF
     36  6b97		       b1 da		      lda	(tt_ptr),y
     37  6b99
     38  6b99
     39  6b99
     40  6b99		       d0 06		      bne	.noEndOfPattern
     41  6b9b
     42  6b9b		       95 d4		      sta	tt_cur_note_index_c0,x
     43  6b9d		       f6 d2		      inc	tt_cur_pat_index_c0,x
     44  6b9f		       d0 d1		      bne	.constructPatPtr
