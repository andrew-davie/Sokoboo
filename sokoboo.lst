     48  6b83				   .constructPatPtr
     49  6b83					      ldy	tt_cur_pat_index_c0,x	; get current pattern (index into tt_SequenceTable)
     50  6b83					      lda	tt_SequenceTable,y
     51  6b83					      IF	TT_USE_GOTO = 1
     52  6b83					      bpl	.noPatternGoto
     53  6b83					      and	#%01111111	; mask out goto bit to get pattern number
     54  6b83					      sta	tt_cur_pat_index_c0,x	; store goto'ed pattern index
     55  6b83					      bpl	.constructPatPtr	; unconditional
     56  6b83				   .noPatternGoto
     57  6b83					      ENDIF
     58  6b83					      tay
     59  6b83					      lda	tt_PatternPtrLo,y
     60  6b83					      sta	tt_ptr
     61  6b83					      lda	tt_PatternPtrHi,y
     62  6b83					      sta	tt_ptr+1
     63  6b83							; get new note
     64  6b83					      IF	TT_USE_OVERLAY = 0
     65  6b83					      ldy	tt_cur_note_index_c0,x
     66  6b83					      ELSE
     67  6b83							; If the V flag is set and if the new note is an instrument,
     68  6b83							; it means it got pre-fetched by an overlay percussion, it has
     69  6b83							; to remain in sustain.
     70  6b83					      clv
     71  6b83							; check if note had been pre-fetched by overlay perc already
     72  6b83					      lda	tt_cur_note_index_c0,x
     73  6b83					      bpl	.notPrefetched
     74  6b83							; If so, remove flag
     75  6b83					      and	#%01111111
     76  6b83					      sta	tt_cur_note_index_c0,x
     77  6b83							; Set V flag for later
     78  6b83					      bit	tt_Bit6Set
     79  6b83				   .notPrefetched
     80  6b83					      tay
     81  6b83					      ENDIF
     82  6b83					      lda	(tt_ptr),y
