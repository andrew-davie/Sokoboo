      0  d063					      DEFINE_SUBROUTINE	CopyROMShadowToRAM	; in INITBANK
      1  d063		       00 1a	   BANK_CopyROMShadowToRAM =	_CURRENT_BANK
      2  d063					      SUBROUTINE
      3  d063				   CopyROMShadowToRAM
    117  d063
    118  d063							; Function copies a RAM prototype ROM bank into the destination RAM bank.  Typically
    119  d063							; the variable definitions are in the ROM shadow because this allows auto-initialisation
    120  d063							; of the variable contents from ROM declarations, but still allows access to them as
    121  d063							; variables when the correct RAM bank is switched in.	Code is also, of course, copied
    122  d063							; into the RAM destination so that code is callable whenever the ROM *or* RAM bank is
    123  d063							; switched in.  Further, if multiple copies are made to multiple RAM banks, then the
    124  d063							; code co-lives in all banks and may run even as bankswitching occurs between those
    125  d063							; banks -- by the very code itself.
    126  d063
    127  d063							; Note: Relies on ROM_Bank having being set via CALL mechanism to call this function
    128  d063		       85 bd		      sta	Board_AddressR+1
    129  d065		       86 dc		      stx	O_ROM_Source_Bank	; source bank
    130  d067		       84 c1		      sty	RAM_Bank	; destination bank
    131  d069
    132  d069		       a0 00		      ldy	#0
    133  d06b		       84 bc		      sty	Board_AddressR
    134  d06d		       84 be		      sty	Board_AddressW
    135  d06f		       a9 14		      lda	#>($1000+RAM_WRITE)
    136  d071		       85 bf		      sta	Board_AddressW+1
    137  d073
    138  d073							; Iterate 4 pages (1K) for complete bank copy
    139  d073
    140  d073		       a9 04		      lda	#4
    141  d075		       85 db		      sta	O_CopyCount
    142  d077
    143  d077		       84 dd	   CopyPage   sty	O_Index
    144  d079		       a5 dc		      lda	O_ROM_Source_Bank
    145  d07b		       20 0f f8 	      jsr	GetROMByte	; get byte from ROM shadow bank
    146  d07e							;tax
    147  d07e		       a4 dd		      ldy	O_Index
    148  d080		       a6 c1		      ldx	RAM_Bank
    149  d082		       20 1d f8 	      jsr	PutBoardCharacter	;6+21(A)	 write byte to RAM bank
    150  d085
    151  d085		       a4 dd		      ldy	O_Index
    152  d087		       c8		      iny
    153  d088		       d0 ed		      bne	CopyPage
    154  d08a
    155  d08a		       e6 bd		      inc	Board_AddressR+1
    156  d08c		       e6 bf		      inc	Board_AddressW+1
    157  d08e
    158  d08e		       c6 db		      dec	O_CopyCount
    159  d090		       d0 e5		      bne	CopyPage
    160  d092
    161  d092		       a4 c1		      ldy	RAM_Bank	; TODO: remove!?
    162  d094		       60		      rts
