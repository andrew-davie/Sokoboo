    102  6b58		       10 6e		      bpl	.noNewNote
    103  6b5a
    104  6b5a							; Timer ran out: Do sequencer
    105  6b5a							; Advance to next note
    106  6b5a		       a2 01		      ldx	#1	; 2 channels
    107  6b5c				   .advanceLoop
    108  6b5c					      IF	TT_USE_OVERLAY = 1
    109  6b5c		       20 72 f3 	      jsr	tt_FetchNote
    110  6b5f				  -	      ELSE
    111  6b5f				  -	      TT_FETCH_CURRENT_NOTE
    112  6b5f					      ENDIF
    113  6b5f							; Parse new note from pattern
    114  6b5f		       c9 10		      cmp	#TT_INS_PAUSE
    115  6b61					      IF	TT_USE_SLIDE = 0
    116  6b61		       90 55		      bcc	.finishedNewNote
    117  6b63		       d0 3d		      bne	.newNote
    118  6b65				  -	      ELSE
    119  6b65				  -	      beq	.pause
    120  6b65				  -	      bcs	.newNote
    121  6b65				  -
    122  6b65				  -			; --- slide/hold ---
    123  6b65				  -			; Adjust frequency and hold note in sustain.
    124  6b65				  -			; composer/tracker has to make sure that no unwanted
    125  6b65				  -			; under/overflow happens.
    126  6b65				  -			; Note: f = f + (8-(16-x)) = x + f - 8
    127  6b65				  -	      adc	tt_cur_ins_c0,x	; carry is clear after cmp
    128  6b65				  -	      sec
    129  6b65				  -	      sbc	#8
    130  6b65				  -	      sta	tt_cur_ins_c0,x
    131  6b65				  -	      bcs	.finishedNewNote	; unconditional, since legally no underflow can happen (ins>0 or HOLD for ins=0)
    132  6b65					      ENDIF
    133  6b65
    134  6b65							; --- pause ---
    135  6b65				   .pause
    136  6b65							; Get release index for current instrument. Since a pause can
    137  6b65							; only follow an instrument, we don't need to handle percussion
    138  6b65							; or commands.
    139  6b65		       b5 d8		      lda	tt_cur_ins_c0,x
    140  6b67		       20 00 f4 	      jsr	tt_CalcInsIndex
    141  6b6a		       b9 42 f4 	      lda	tt_InsReleaseIndexes-1,y	; -1 b/c instruments start at #1
