     98  6b76				   tt_Player  SUBROUTINE
     99  6b76							; ==================== Sequencer ====================
    100  6b76							; Decrease speed timer
    101  6b76		       c6 c7		      dec	tt_timer
    102  6b78		       10 6e		      bpl	.noNewNote
    103  6b7a
    104  6b7a							; Timer ran out: Do sequencer
    105  6b7a							; Advance to next note
    106  6b7a		       a2 01		      ldx	#1	; 2 channels
    107  6b7c				   .advanceLoop
    108  6b7c					      IF	TT_USE_OVERLAY = 1
    109  6b7c		       20 92 f3 	      jsr	tt_FetchNote
    110  6b7f				  -	      ELSE
    111  6b7f				  -	      TT_FETCH_CURRENT_NOTE
    112  6b7f					      ENDIF
    113  6b7f							; Parse new note from pattern
    114  6b7f		       c9 10		      cmp	#TT_INS_PAUSE
    115  6b81					      IF	TT_USE_SLIDE = 0
    116  6b81		       90 55		      bcc	.finishedNewNote
    117  6b83		       d0 3d		      bne	.newNote
    118  6b85				  -	      ELSE
    119  6b85				  -	      beq	.pause
    120  6b85				  -	      bcs	.newNote
    121  6b85				  -
