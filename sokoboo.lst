     27  7c6b
     28  7c6b							; PLANNED PLAYER VARIANTS:
     29  7c6b							; - RAM, speed, player ROM: c0/c1 patterns have same length
     30  7c6b							; - RAM: Pack 2 values (out of cur_pat_index, cur_note_index, envelope_index)
     31  7c6b							;	 into one and use lsr/asl to unpack them, allowing only ranges of
     32  7c6b							;	 16/16 or 32/8 for them, depending on number of patterns, max
     33  7c6b							;	 pattern size and max ADSR size
     34  7c6b							; - ROM: Check if tt_SequenceTable can hold ptrs directly without indexing
     35  7c6b							;	 tt_PatternPtrLo/Hi. Can be smaller if not many patterns get repeated
     36  7c6b							;	 (saves table and decode routine)
     37  7c6b							; - Speed: Inline tt_CalcInsIndex
     38  7c6b							; - Speed: Store ptr to current note in RAM instead of reconstructing it?
     39  7c6b							;	 Might also save the need for cur_note_index
     40  7c6b
     41  7c6b
     42  7c6b							; ---------------------------------------------------------------------
     43  7c6b							; Helper macro: Retrieves current note. May advance pattern if needed.
     44  7c6b							; Becomes a subroutine if TT_USE_OVERLAY is used.
     45  7c6b							; ---------------------------------------------------------------------
     46  7c6b					      MAC	tt_fetch_current_note
     47  7c6b							; construct ptr to pattern
     48  7c6b				   .constructPatPtr
     49  7c6b					      ldy	tt_cur_pat_index_c0,x	; get current pattern (index into tt_SequenceTable)
     50  7c6b					      lda	tt_SequenceTable,y
     51  7c6b					      IF	TT_USE_GOTO = 1
     52  7c6b					      bpl	.noPatternGoto
     53  7c6b					      and	#%01111111	; mask out goto bit to get pattern number
     54  7c6b					      sta	tt_cur_pat_index_c0,x	; store goto'ed pattern index
     55  7c6b					      bpl	.constructPatPtr	; unconditional
     56  7c6b				   .noPatternGoto
     57  7c6b					      ENDIF
     58  7c6b					      tay
     59  7c6b					      lda	tt_PatternPtrLo,y
     60  7c6b					      sta	tt_ptr
     61  7c6b					      lda	tt_PatternPtrHi,y
     62  7c6b					      sta	tt_ptr+1
     63  7c6b							; get new note
     64  7c6b					      IF	TT_USE_OVERLAY = 0
     65  7c6b					      ldy	tt_cur_note_index_c0,x
     66  7c6b					      ELSE
     67  7c6b							; If the V flag is set and if the new note is an instrument,
     68  7c6b							; it means it got pre-fetched by an overlay percussion, it has
     69  7c6b							; to remain in sustain.
     70  7c6b					      clv
     71  7c6b							; check if note had been pre-fetched by overlay perc already
     72  7c6b					      lda	tt_cur_note_index_c0,x
     73  7c6b					      bpl	.notPrefetched
     74  7c6b							; If so, remove flag
     75  7c6b					      and	#%01111111
     76  7c6b					      sta	tt_cur_note_index_c0,x
     77  7c6b							; Set V flag for later
     78  7c6b					      bit	tt_Bit6Set
     79  7c6b				   .notPrefetched
     80  7c6b					      tay
     81  7c6b					      ENDIF
     82  7c6b					      lda	(tt_ptr),y
     83  7c6b							; pre-process new note
     84  7c6b							; 7..5: instrument (1..7), 4..0 (0..31): frequency
     85  7c6b							; 0/0: End of pattern
     86  7c6b					      bne	.noEndOfPattern
     87  7c6b							; End of pattern: Advance to next pattern
     88  7c6b					      sta	tt_cur_note_index_c0,x	; a is 0
     89  7c6b					      inc	tt_cur_pat_index_c0,x
     90  7c6b					      bne	.constructPatPtr	; unconditional
     91  7c6b				   .noEndOfPattern
     92  7c6b					      ENDM
