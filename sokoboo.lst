     28  6b8f							; PLANNED PLAYER VARIANTS:
     29  6b8f							; - RAM, speed, player ROM: c0/c1 patterns have same length
     30  6b8f							; - RAM: Pack 2 values (out of cur_pat_index, cur_note_index, envelope_index)
     31  6b8f							;	 into one and use lsr/asl to unpack them, allowing only ranges of
     32  6b8f							;	 16/16 or 32/8 for them, depending on number of patterns, max
     33  6b8f							;	 pattern size and max ADSR size
     34  6b8f							; - ROM: Check if tt_SequenceTable can hold ptrs directly without indexing
     35  6b8f							;	 tt_PatternPtrLo/Hi. Can be smaller if not many patterns get repeated
     36  6b8f							;	 (saves table and decode routine)
     37  6b8f							; - Speed: Inline tt_CalcInsIndex
     38  6b8f							; - Speed: Store ptr to current note in RAM instead of reconstructing it?
     39  6b8f							;	 Might also save the need for cur_note_index
     40  6b8f
     41  6b8f
     42  6b8f							; ---------------------------------------------------------------------
     43  6b8f							; Helper macro: Retrieves current note. May advance pattern if needed.
     44  6b8f							; Becomes a subroutine if TT_USE_OVERLAY is used.
     45  6b8f							; ---------------------------------------------------------------------
     46  6b8f					      MAC	tt_fetch_current_note
     47  6b8f							; construct ptr to pattern
     48  6b8f				   .constructPatPtr
     49  6b8f					      ldy	tt_cur_pat_index_c0,x	; get current pattern (index into tt_SequenceTable)
     50  6b8f					      lda	tt_SequenceTable,y
     51  6b8f					      IF	TT_USE_GOTO = 1
     52  6b8f					      bpl	.noPatternGoto
     53  6b8f					      and	#%01111111	; mask out goto bit to get pattern number
     54  6b8f					      sta	tt_cur_pat_index_c0,x	; store goto'ed pattern index
     55  6b8f					      bpl	.constructPatPtr	; unconditional
     56  6b8f				   .noPatternGoto
     57  6b8f					      ENDIF
     58  6b8f					      tay
     59  6b8f					      lda	tt_PatternPtrLo,y
     60  6b8f					      sta	tt_ptr
     61  6b8f					      lda	tt_PatternPtrHi,y
     62  6b8f					      sta	tt_ptr+1
     63  6b8f							; get new note
     64  6b8f					      IF	TT_USE_OVERLAY = 0
