     12  6bad		       a8		      tay
     13  6bae		       b9 79 f5 	      lda	tt_PatternPtrLo,y
     14  6bb1		       85 db		      sta	tt_ptr
     15  6bb3		       b9 7e f5 	      lda	tt_PatternPtrHi,y
