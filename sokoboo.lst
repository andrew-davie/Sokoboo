    167  d095
    168  d095							; Now modify the hardwired colours so that we're correctly switched for NTSC/PAL
    169  d095							; The platform (0=NTSC, 1=PAL) is set from the right difficulty switch
    170  d095							; Note: This relies on DrawTheScreen starting on page boundary so that the (),y
    171  d095							; addressing will not violate the page-crossing restriction of 3E.
    172  d095
    173  d095							;		  sty RAM_Bank			  ; we assume we called CopyROMShadowToRAM before
    174  d095
    175  d095		       a9 00		      lda	#<DrawTheScreen	; = 0
    176  d097		       85 be		      sta	Board_AddressW
    177  d099		       a9 f4		      lda	#>( DrawTheScreen + RAM_WRITE )
    178  d09b		       85 bf		      sta	Board_AddressW+1
    179  d09d
    180  d09d							; first, set the x index (with last one being a RTS ($60))
    181  d09d
